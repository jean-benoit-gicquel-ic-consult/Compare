              </ActionSequence>
            </ServerActionCall>
          </Button>
        </ContainerTemplate>
      </ControlList>
      <Tables ScriptItemUID="Tables1" />
      <Controls ScriptItemUID="Controls1" />
      <Functions ScriptItemUID="Functions1" />
      <DataEventHandlers ScriptItemUID="DataEventHandlers1" />
    </Context>
    <ComponentInterface ScriptItemUID="ComponentInterface1" />
