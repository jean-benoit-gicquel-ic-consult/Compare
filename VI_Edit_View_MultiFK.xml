<WebDesigner xmlns:ns0="http://www.vi-net.de/aeds" ns0:tdnsHash="2c6bb4c4-a28e-17ce-d676-e8ec419a853e">
  <GlobalControl MetamodelVersion="2016-05-08" ID="VI_Edit_View_MultiFK">
    <ContextConfiguration>
