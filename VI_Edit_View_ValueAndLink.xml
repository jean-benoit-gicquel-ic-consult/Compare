      <ControlList ScriptItemUID="ControlList1">
        <LinkContainer ScriptItemUID="LinkContainer1" EnabledCondition="not(isnullorempty(GetTargetUrl()))&#xD;&#xA;AND EnableNavigation()&#xD;&#xA;AND not(IsNoLink())">
