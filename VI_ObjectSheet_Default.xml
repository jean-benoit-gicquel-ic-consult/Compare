        <Column Name="XObjectKey" DataType="System.String" ScriptItemUID="Column4" DataSource="Expression" Expression="try(objectwalker(&quot;XObjectKey&quot;), getobjectkey())" />
      </VirtualTableExtension>
    </ComponentInterface>
  </GlobalControl>
</WebDesigner>
