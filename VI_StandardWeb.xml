        <MenuItem ID="QER_GovernanceAdministration" ScriptItemUID="CAPMenuItem1" Title="translate(&quot;#LDS#Governance Administration&quot;)" ContextID="QER_GovernanceAdministration" SortOrder="20" Condition="(IsPreProp('RISKINDEX')&#xD;&#xA;and&#xD;&#xA;exists(&quot;personinaerole&quot;,&#xD;&#xA;sqland(&#xD;&#xA;sqlcompareuid(&quot;uid_person&quot;, getuser()),&#xD;&#xA;&quot;xorigin &gt; 0 and uid_aerole in (&#xD;&#xA;select uid_aerole from AERole where UID_AERole in ('CAP-AEROLE-IAG-CISO', 'CAP-AEROLE-AUDITING-AUDITOR') and uid_DialogGroup in (select uid_dialoggroup from DialogGroup where isdeactivatedbypreprocessor = 0)&#xD;&#xA;)&quot;&#xD;&#xA;)))&#xD;&#xA;&#xD;&#xA;OR&#xD;&#xA;(IsPreProp('COMPLIANCE') and exists(&quot;compliancearea&quot;, format(&quot;uid_orgresponsible in ( select uid_aerole from aerolecollection where uid_parentaerole in (select uid_aerole from personinaerole where xorigin &gt; 0 and uid_person = '{0}' ))&quot;, from user select uid_person)))&#xD;&#xA;&#xD;&#xA;OR&#xD;&#xA;(-- visible for Compliance &amp; Security Officer&#xD;&#xA;IsPreProp('COMPLIANCE') and (&#xD;&#xA;exists(&quot;personinaerole&quot;, variable(&quot;xorigin &gt; 0 and uid_aerole = 'CAP-AEROLE-IAG-CISO' and uid_person = '%useruid%'&quot;))&#xD;&#xA;or&#xD;&#xA;-- visible for Auditor&#xD;&#xA;exists(&quot;personinaerole&quot;, variable(&quot;xorigin &gt; 0 and uid_aerole = 'CAP-AEROLE-AUDITING-AUDITOR' and uid_person = '%useruid%'&quot;))&#xD;&#xA;or&#xD;&#xA;-- Whereclause for: ComplianceArea.UID_OrgResponsible, ComplianceRule.UID_OrgResponsible, ComplianceRule.UID_OrgRuler, Department-Responsible&#xD;&#xA;(exists(&quot;person&quot;, format(&quot;uid_person = {1} and (uid_person in ( select piae.UID_Person from PersonInAERole piae join AERoleCollection aec on aec.UID_ParentAERole = piae.UID_AERole&#xD;&#xA;where piae.UID_AERole in ( select cr.uid_orgruler from ComplianceRule cr where ({0}))&#xD;&#xA;or piae.UID_AERole in ( select cr.uid_orgresponsible from ComplianceRule cr where ({0}))&#xD;&#xA;or piae.UID_AERole in ( select ca.uid_orgresponsible from compliancearea ca ))&#xD;&#xA;or UID_Person in ( select hho.UID_PersonHead from HelperHeadOrg hho join Department dep on dep.UID_Department = hho.UID_Org))&quot;&#xD;&#xA;, sqlor(sqlcompare(&quot;cr.isinactive&quot;,false,&quot;bool&quot;,&quot;equal&quot;),sqlcompare(&quot;cr.isworkingcopy&quot;,false,&quot;bool&quot;,&quot;equal&quot;)), sqlformatvalue(getuser(),&quot;string&quot;, &quot;NonUnicodeLiterals&quot;)&#xD;&#xA;)))))&#xD;&#xA;&#xD;&#xA;OR&#xD;&#xA;(-- visible for Compliance &amp; Security Officer&#xD;&#xA;IsPreProp(&quot;QERPOLICY&quot;) and (&#xD;&#xA;exists(&quot;personinaerole&quot;, variable(&quot;xorigin &gt; 0 and uid_aerole = 'CAP-AEROLE-IAG-CISO' and uid_person = '%useruid%'&quot;))&#xD;&#xA;or&#xD;&#xA;-- visible for Auditor&#xD;&#xA;exists(&quot;personinaerole&quot;, variable(&quot;xorigin &gt; 0 and uid_aerole = 'CAP-AEROLE-AUDITING-AUDITOR' and uid_person = '%useruid%'&quot;))&#xD;&#xA;or&#xD;&#xA;-- Whereclause for: ComplianceArea.UID_OrgResponsible, QERPolicy.UID_AERoleResponsible, QERPolicy.UID_AERoleRuler&#xD;&#xA;(exists(&quot;person&quot;, format(&quot;uid_person = {1} and (uid_person in ( select piae.UID_Person from PersonInAERole piae join AERoleCollection aec on aec.UID_ParentAERole = piae.UID_AERole&#xD;&#xA;where piae.xorigin &gt; 0 and piae.UID_AERole in ( select cr.uid_aeroleruler from qerpolicy cr where ({0}))&#xD;&#xA;or piae.UID_AERole in ( select cr.uid_aeroleresponsible from qerpolicy cr where ({0}))&#xD;&#xA;or piae.UID_AERole in ( select ca.uid_orgresponsible from compliancearea ca ))&#xD;&#xA;)&quot;&#xD;&#xA;, sqlor(sqlcompare(&quot;cr.isinactive&quot;,false,&quot;bool&quot;,&quot;equal&quot;),sqlcompare(&quot;cr.isworkingcopy&quot;,false,&quot;bool&quot;,&quot;equal&quot;)), sqlformatvalue(getuser(),&quot;string&quot;, &quot;NonUnicodeLiterals&quot;)&#xD;&#xA;)))))&#xD;&#xA;&#xD;&#xA;OR&#xD;&#xA;(IsPreProp(&quot;COMPLIANCE&quot;) and IsPreProp(&quot;SAPR3&quot;) and IsPreProp(&quot;SAPRights&quot;)&#xD;&#xA;and&#xD;&#xA;exists(&quot;compliancearea&quot;, &quot;uid_compliancearea in (&#xD;&#xA;select ria.uid_compliancearea from complianceruleinarea ria&#xD;&#xA;join compliancerule cr on &quot;+&#xD;&#xA;sqland(sqlcompare(&quot;cr.IsInactive&quot;,false,&quot;bool&quot;,&quot;equal&quot;),&#xD;&#xA;sqlcompare(&quot;cr.IsWorkingCopy&quot;, false, &quot;bool&quot;, &quot;equal&quot;))&#xD;&#xA;+&quot; and cr.uid_compliancerule = ria.uid_compliancerule&#xD;&#xA;join aerolecollection col on col.uid_aerole = cr.uid_orgresponsible&#xD;&#xA;join personinaerole pio on pio.uid_aerole = col.uid_parentaerole and pio.uid_person = '&quot; +&#xD;&#xA;select uid_person from user + &quot;')&quot;))&#xD;&#xA;&#xD;&#xA;OR&#xD;&#xA;(IsPreProp(&quot;COMPLIANCE&quot;) and IsPreProp(&quot;SAPR3&quot;) and IsPreProp(&quot;SAPRights&quot;) and&#xD;&#xA;exists(&quot;personinaerole&quot;,&#xD;&#xA;sqland(&#xD;&#xA;sqlcompareuid(&quot;uid_person&quot;, getuser()),&#xD;&#xA;&quot;xorigin &gt; 0 and uid_aerole in ( select uid_aerole from aerolecollection where uid_parentaerole = 'SAC-AEROLE-RULEADMIN-SAPRIGHTS')&quot;&#xD;&#xA;)))&#xD;&#xA;">
